from snakemake.utils import R

rule table_CNV_analysis:
	"""Output regions ouside confidence to suggest CNV in tables"""
	input:
		expand("results/coverage/{{mapper}}{{parameterSet}}/{{ref}}/{{kind}}/{sample}_20_20.cov", sample = config["samples"].keys() )
	output:
		expand("results/CNV/{{mapper}}{{parameterSet}}/{{ref}}/{{kind}}/{sample}_CNV.txt", sample = config["samples"].keys() )
	threads: 48
	script: "CNV.R"
